{
  "expo": {
    "name": "Penelope",
    "slug": "penelope-creative-assistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/penelope-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/penelope_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1E26"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "githubUrl":"https://github.com/TyrelN/penelope",

    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier":"com.tyapps.penelope",
      "buildNumber":"1.0.0",
      "supportsTablet": true,
      "userInterfaceStyle": "dark"
    },
    "android": {
      "package":"com.tyapps.penelope",
      "versionCode":1,
      "softwareKeyboardLayoutMode": "pan",
      "userInterfaceStyle": "dark",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/penelope-logo.png",
        "backgroundColor": "#1C1E26"
      }
    },
    "androidStatusBar":{
      "barStyle": "dark-content",
      "backgroundColor": "#F09383"
    },
    "notification":{
        "icon": "./assets/images/penelope-logo.png",
        "color": "#1C1E26"
    },
    "web": {
      "favicon": "./assets/images/penelope_favicon.png"
    }
  }
}
